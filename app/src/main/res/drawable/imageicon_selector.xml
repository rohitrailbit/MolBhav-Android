<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#E0E0E0" /> <!-- Change to the desired pressed color -->
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@android:color/transparent" /> <!-- Default state, transparent background -->
        </shape>
    </item>
</selector>
